{
    "name": "takshak/adash",
    "keywords": ["laravel", "breeze", "admin panel", "panel", "dashboard"],
    "description": "Simple admin panel starter kit",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Takshak Tiwari",
            "email": "takshaktiwari@gmail.com"
        }
    ],
    "require": {
        "laravel/breeze": "1.20.0",
        "intervention/image": "^2.7",
        "takshak/alertt": "^1.0",
        "takshak/imager": "^1.0"
    },
    "autoload": {
        "psr-4" : {
            "Takshak\\Adash\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Takshak\\Adash\\Providers\\AdashServiceProvider"
            ]
        }
    }
}
